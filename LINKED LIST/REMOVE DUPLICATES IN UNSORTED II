import java.util.HashSet;
import java.util.Set;

class Solution {
    public Node removeDuplicates(Node head) {
        if (head == null) return null;

        Set<Integer> seen = new HashSet<>();
        Node cur = head;
        Node prev = null;

        while (cur != null) {
            if (seen.contains(cur.data)) {
                prev.next = cur.next;
            } else {
                seen.add(cur.data);
                prev = cur;
            }
            cur = cur.next;
        }

        return head;
    }
}
