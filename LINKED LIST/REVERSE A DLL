/*
class Node {
    int data;
    Node next;
    Node prev;

    Node(int data) {
        this.data = data;
        this.next = null;
        this.prev = null;
    }
}
*/
class Solution {
    public Node reverse(Node head) {
        if(head == null || head.next == null){
            return head;
        }
        Node pre = null;
        Node cur = head;
        while(cur!=null){
            Node ne = cur.next;
            cur.next = pre;
            cur.prev = ne;
            pre = cur;
            cur = ne;
        }
        return pre;
    }
}
