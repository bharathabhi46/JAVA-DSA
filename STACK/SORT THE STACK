class Solution {
    public static void pB(int da, Stack<Integer> st){
        if(st.isEmpty() || st.peek()<=da){
            st.push(da);
            return;
        }
        int top = st.pop();
        pB(da, st);
        st.push(top);
    }
    public void sortStack(Stack<Integer> st) {
         if(st.isEmpty()){
             return;
         }
         int top = st.pop();
         sortStack(st);
         pB(top, st);
    }
}
